{
  "name": "vscode-connect",
  "displayName": "vscode-connect",
  "description": "Connect to vscode editor using a websocket.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["onStartupFinished", "onLanguage:lua"],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-connect.getAllFilesAndFolders",
